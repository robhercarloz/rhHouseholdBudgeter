                            <ul class="nav">
                                <li>
                                    <a href="#">
                                        <span class="sidebar-mini-icon">R</span>
                                        <span class="sidebar-normal"> Reports </span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span class="sidebar-mini-icon">IE</span>
                                        <span class="sidebar-normal"> Incomes/Expenses </span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </li>
                    <li>
                        <a href="#">
                            <i class="nc-icon nc-email-85"></i>
